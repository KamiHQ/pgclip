From: <%= @result.created_at %> , took <%= @result.execution_time %>s

<table>
<thead>
<tr>
<% @fields.each do |field| %>
  <th>
    <%= field %>
  </th>
<% end %>
</tr>
</thead>
<tbody>
<% @values.each do |value| %>
<tr>
  <% value.each do |field| %>
    <td>
      <%= field %>
    </td>
  <% end %>
</tr>
<% end %>
</tbody>
</table>

